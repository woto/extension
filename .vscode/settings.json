{
    "typescript.tsdk": "./node_modules/typescript/lib",
    "files.eol": "\n",
    "json.schemas": [
        {
            "fileMatch": [
                "/manifest.json"
            ],
            "url": "http://json.schemastore.org/chrome-manifest"
        }
    ],
    "workbench.colorTheme": "Quiet Light",
    "files.watcherExclude": {
        "**/dist": true,
        "**/dist/**": true,
        "dist": true,
        "dist/**": true
    },
    "files.exclude": {
        "**/dist": true,
        "**/dist/**": true,
        "dist": true,
        "dist/**": true
    },
    "search.exclude": {
        "**/dist": true,
        "**/dist/**": true,
        "dist": true,
        "dist/**": true
    }
}
